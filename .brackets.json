{
    "spaceUnits": 2,
    "insertHintOnTab": true,
    "closeBrackets": true,
    "linting.enabled": true,
    "language": {
        "javascript": {
            "linting.prefer": "ESLint",
            "linting.usePreferredOnly": true
        }
    },
    "jwolfe.file-tree-exclude.list": [
        "node_modules",
        "obigo_lib",
        "obigo_update"
    ],
    "obigo-studio.app-tools.signatureCertificate": "OB.Obigo_new",
    "obigo-studio.app-tools.cmsCertificate": "Assim",
    "obigo-studio.app-tools.signatureAccessLevel": "level-1",
    "obigo-studio.app-tools.buildOption": "build",
    "obigo-studio.app-tools.contentsEncrypt": false,
    "obigo-studio.app-tools.runTarget": "simulator.OAD_1",
    "obigo-studio.app-tools.installTarget": "",
    "obigo-studio.app-tools.remoteDebuggingTarget": "",
    "obigo-studio.app-tools.buildMode": "development"
}